{
  "buildCommand": "echo 'Static site ready'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/kektok",
      "destination": "/kektok/SITE_INTERFACEFINALPROJECT/index.html"
    },
    {
      "source": "/flight",
      "destination": "/folderflight/FLIGHTCLAUDE1/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}